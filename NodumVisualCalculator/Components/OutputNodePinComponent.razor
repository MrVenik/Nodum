@if (VisualNode.OutputNodePin.Showed)
{
    <img id="@VisualNode.OutputNodePin.ElementId" src="/image/outputnode.png" class="output-pin" @onclick="@(args => OnNodePinClick(args, VisualNode))">
}

@code {
    [Parameter]
    public VisualNode VisualNode { get; set; }
    [Parameter]
    public Action<MouseEventArgs, VisualNode> OnNodePinClick { get; set; }
}
