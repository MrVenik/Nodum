@code {
    [CascadingParameter]
    public SidebarTab SidebarTab { get; set; }

    [Parameter]
    public RenderFragment ChildContent { get; set; }

    [Parameter]
    public Action<MouseEventArgs> OnClick { get; set; }

    protected override void OnInitialized()
    {
        if (SidebarTab != null)
        {
            SidebarTab.Elements.Add(this);
        }
    }
}
